    function drawChart() {

      var data = new google.visualization.DataTable();
      data.addColumn('string', 'Topping');
      data.addColumn('number', 'Slices');
      data.addRows([
        ['Ensino fundamental incompleto', 33.1],
        ['Nenhuma instrução formal', 6.9],
        ['Esino fundamental incompleto', 8.1],
        ['Ensino médio incompleto', 4.5],
        ['Ensino básico completo', 47.4]
      ]);

      var options = {
        'title': 'Porcentagem de conclusão do Ensino Básico (até o ensino médio)',
        'width': 550,
        'height': 300,
        'backgroundColor': 'transparent'
      };

      var chart = new google.visualization.PieChart(document.getElementById('chart_div'));
      chart.draw(data, options);

    }
